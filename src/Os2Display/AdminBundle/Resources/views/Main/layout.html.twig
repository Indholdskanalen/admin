{% extends '::base.html.twig' %}

{% block title %}{{ site_title }}{% endblock %}

{% block meta %}
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width">
  <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
{% endblock %}

{% block stylesheets %}
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

  <link rel="stylesheet" href="{{ asset('bundles/os2displayadmin/assets/build/styles.min.css?' ~ version) }}">

  <!-- Css from styleguide (Scss files not found in this project) -->
  <link rel="stylesheet" href="{{ asset('bundles/os2displayadmin/css/styles-new.css?' ~ version) }}">
{% endblock %}
